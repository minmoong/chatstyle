datasource db {
  provider = "postgresql"
  // url = env("DATABASE_URL")
  url = "postgres://gitpod@localhost/regionscore"
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

model RegionScore {
  uid String @id @default(cuid())
  region String
  score BigInt @default(0)
}